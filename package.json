{
  "name": "get-selenium",
  "version": "1.0.1",
  "description": "Make sure you've got the selenium jar and the chromedriver executable",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/. --ignore test/ src/",
    "lint": "eslint src",
    "prepublish": "npm run build",
    "test": "cross-env NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha --require ./test/setup.js test/",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivarni/get-selenium.git"
  },
  "author": "Ivar Nilsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivarni/get-selenium/issues"
  },
  "homepage": "https://github.com/ivarni/get-selenium#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "extract-zip": "^1.5.0",
    "file-exists": "^1.0.0",
    "md5-file": "^3.1.1",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "coveralls": "^2.11.11",
    "cross-env": "^2.0.0",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "expect.js": "^0.3.1",
    "glob": "^7.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^7.0.0",
    "tmp": "0.0.28"
  },
  "files": [
    "lib/"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
